{
    "name": "modus/skeleton",
    "description": "A skeleton project for the Modus framework.",
    "minimum-stability": "dev",
    "require": {
        "modus/framework": "dev-router"
    },
    "require-dev": {
        "symfony/var-dumper": "2.6.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Brandon Savage",
            "email": "brandon@brandonsavage.net"
        }
    ],
    "autoload": {
        "psr-0": {
            "AppConfig": "config/"
        },
        "psr-4": {
            "Example\\": "src/"
        }
    },
    "suggest": {
        "thephpleague/flysystem": "Use Flysystem for file system manipulation",
        "codeguy/Upload": "Use Upload for uploading files"
    }
}
